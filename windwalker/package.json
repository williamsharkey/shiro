{
  "name": "windwalker",
  "version": "0.1.0",
  "description": "Unified test suite for Spirit on Foam and Shiro cloud OS environments",
  "type": "module",
  "scripts": {
    "test": "node tests/runner-linkedom.js",
    "test:linkedom": "node tests/runner-linkedom.js",
    "test:hotreload": "npx tsx tests/level-12-hotreload/hotreload-node.test.js",
    "test:selfbuild": "ENABLE_NETWORK_TESTS=1 node tests/runner-linkedom.js",
    "test:skyeyes": "node tests/runner-skyeyes.js",
    "test:skyeyes:foam": "OS_TARGET=foam node tests/runner-skyeyes.js",
    "test:skyeyes:shiro": "OS_TARGET=shiro node tests/runner-skyeyes.js",
    "test:all": "node tests/runner-linkedom.js && npx tsx tests/level-12-hotreload/hotreload-node.test.js"
  },
  "dependencies": {
    "fake-indexeddb": "^6.0.0",
    "isomorphic-git": "^1.36.3",
    "linkedom": "^0.18.0"
  },
  "license": "MIT"
}
